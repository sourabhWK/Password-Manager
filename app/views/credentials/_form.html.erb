<%= form_with model: credential do |form| %>
  <div>
    <%= form.label :website %><br>
    <%= form.text_field :website %>
    <% credential.errors.full_messages_for(:website).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :username %><br>
    <%= form.text_field :username %><br>
    <% credential.errors.full_messages_for(:username).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %><br>
    <% credential.errors.full_messages_for(:password).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
  
<% end %>
